<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="MNBCurrentExchangeRates" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="MNBCurrentExchangeRates" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="Day">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Rate" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:simpleContent msdata:ColumnName="Rate_Text" msdata:Ordinal="2">
                    <xs:extension base="xs:string">
                      <xs:attribute name="unit" type="xs:string" />
                      <xs:attribute name="curr" type="xs:string" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="date" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>